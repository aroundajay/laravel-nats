{
  "name": "seddighi78/laravel-nats",
  "description": "Nats jetstream client for Laravel",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "nats",
    "pub-sub",
    "laravel"
  ],
  "homepage": "https://github.com/seddighi78/laravel-nats",
  "autoload": {
    "psr-4": {
      "Seddighi78\\LaravelNats\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Alireza Seddighi",
      "email": "seddighi.alireza78@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "basis-company/nats": "^0.26",
    "illuminate/config": "^10.0 || ^11.0 || ^12.0",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seddighi78\\LaravelNats\\NatsServiceProvider"
      ],
      "aliases": {
        "Nats": "Seddighi78\\LaravelNats\\Facades"
      }
    }
  }
}

